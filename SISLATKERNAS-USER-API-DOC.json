    "/api/v1/logout": {
      "delete": {
        "description": "User logout",
         "parameters": [
          {
            "name": "Authorization",
            "in": "header"
          }
        ],
        "responses": {
          "200": {
            "description": "Sukses logout user",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                      "properties": {
                        "data": {
                          "type": "boolean"
                    },
                    "errors": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/refresh": {
      "post": {
        "description": "Regenerate JWT auth token",
         "parameters": [
          {
            "name": "Authorization",
            "in": "header"
          }
        ],
        "responses": {
          "200": {
            "description": "Sukses regenerate JWT auth token",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "object",
                      "properties": {
                        "access_token": {
                          "type": "string"
                        },
                          "token_type": {
                          "type": "string"
                        },
                         "expires_in": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/users-import": {
      "post": {
        "description": "Impor data user dari file xlsx",
        "parameters": [
          {
            "name": "Authorization",
            "in": "header"
          }
        ],
        "requestBody": {
          "description": "Upload file",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Sukses melakukan impor data user",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                      "properties": {
                        "data": {
                          "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
