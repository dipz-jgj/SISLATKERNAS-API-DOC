            "name": "Authorization",
            "in": "header"
          }
        ],
        "requestBody": {
          "description": "Memperbarui data user",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "nama_lengkap": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  },
                 "id_role": {
                    "type": "array",
                      "items": {
                        "type": "integer"
                      }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Sukses memperbarui data user",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "object",
                      "properties": {
                        "email": {
                          "type": "string"
                        },
                         "password": {
